<Window x:Class="Tours.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tours"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="115*"/>
            <RowDefinition Height="414*"/>
            <RowDefinition Height="40*"/>
        </Grid.RowDefinitions>
        <Grid Background="#bae3e8"></Grid>
        <Image Source="/Resources/ImageLogo.jpg" HorizontalAlignment="Left" Margin="29,24,0,24" Width="109"/>
        <TextBlock Text="Header" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Height="40" Margin="348,37,348,38" Width="96"/>
        <Grid Background="#445c93" Grid.Row="2"></Grid>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"/>
        <Button Content="Back" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click" Height="Auto" Margin="0,54,5,32"/>
    </Grid>
</Window>
